# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/veeam_dashboard
PGUSER=veeam_user
PGPASSWORD=veeam_password
PGDATABASE=veeam_dashboard
PGHOST=localhost
PGPORT=5432

# Session Secret (generate with: openssl rand -hex 32)
SESSION_SECRET=your-secret-key-here

# Veeam API Configuration
# Obtenha a chave em: Configuration > REST API Keys no VSPC
VEEAM_API_URL=https://vspc.example.com:1280
VEEAM_API_KEY=your-veeam-api-key-here

# Application
NODE_ENV=development
PORT=5000
